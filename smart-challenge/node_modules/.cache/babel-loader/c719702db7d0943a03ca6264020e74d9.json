+'-p1'\" >Nome:{{nome}}</p>\r\n                <p :id=\"reference+'-p2'\" >Email:{{email}}</p>\r\n                <p :id=\"reference+'-p3'\" >Idade:{{idade}}</p>\r\n                <p :id=\"reference+'-p4'\" >Budget:{{budget}}</p>\r\n            </div>\r\n        </div>\r\n        \r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"Client\",\r\n        props:{\r\n            imagens: Array,\r\n            nome: String,\r\n            email: String,\r\n            idade: Number,\r\n            budget: String,\r\n            telefone: String,\r\n            companhia: String,\r\n            reference: String\r\n        },\r\n        data(){\r\n            return{\r\n                viewClient: false,\r\n                styleViewClient: {}\r\n            }\r\n        },\r\n        methods:{\r\n            totalInfoClient:function(event){\r\n                console.log(event.target.id.substring(0,8));\r\n                let clientChosen = document.getElementById(event.target.id.substring(0,8));\r\n                console.log(clientChosen)\r\n                if(clientChosen != undefined){\r\n                    if(this.viewClient == false){\r\n                        clientChosen.style.height = \"490px\";\r\n                        this.viewClient = true;\r\n                        this.styleViewClient = {\r\n                            width: \"auto\",\r\n                            height: \"490px\"\r\n                        }\r\n                    }else{\r\n                        clientChosen.style.height = \"190px\";\r\n                        this.viewClient = false;\r\n                        this.styleViewClient = {}\r\n                    }\r\n                }\r\n                \r\n            }\r\n        }         \r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .client{\r\n        display:block;\r\n        padding:3px;\r\n        border:2px solid #2b3a4a;\r\n        background-color: gray;\r\n        width: auto;\r\n        height: 190px;\r\n        margin-bottom: 10px;\r\n        margin-right: 6%;\r\n        margin-left: 10px;\r\n        direction: ltr;\r\n        cursor: pointer;\r\n    }\r\n    .imagem{\r\n        margin-top: 10px;\r\n        width: 30%;\r\n        height: 100px;\r\n        float:left;\r\n    }\r\n    .imagem img{\r\n        border:2px solid #2b3a4a;\r\n        width: 170px;\r\n        height: 170px;\r\n        border-radius: 100%;\r\n        margin-right: 0%;\r\n    }\r\n    .info-client{\r\n        margin-top: 10px;\r\n        float: right;\r\n        width: 60%;\r\n        height: 100px;\r\n    }\r\n    .info-client p{\r\n        margin-right: 100%;\r\n        font-weight: bold;\r\n    }\r\n    .view-client img{\r\n        border:2px solid #2b3a4a;\r\n        width: 80px;\r\n        height: 80px;\r\n        border-radius: 100%;\r\n        margin-right: 10px;\r\n    }\r\n    .view-client-imagem{\r\n        width: 100%;\r\n        background-color: #5fc934;\r\n    }\r\n    .view-info-client{\r\n        background-color: red;\r\n    }\r\n\r\n</style>"],"sourceRoot":"src/components"}]}